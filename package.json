{
  "name": "teleport-docs-next",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "cleanup-serverless": "rm -rf ./.next/server/chunks/static/media",
    "git-update": "git submodule update --remote",
    "build-node": "rm -rf .build && tsc --build tsconfig.node.json && tsc-esm-fix --target='.build' --ext='.mjs'",
    "add-symlinks": "node ./scripts/add-symlinks.mjs",
    "preflight": "yarn build-node && yarn add-symlinks",
    "dev": "yarn preflight && next dev",
    "build": "yarn preflight && next build && yarn cleanup-serverless",
    "update-and-build": "yarn git-update && yarn build",
    "start": "next start",
    "test": "loadr -- uvu",
    "base:eslint": "eslint --cache --cache-location node_modules/.cache/eslint '**/*.{js,jsx,ts,tsx}'",
    "base:prettier": "prettier '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "yarn base:eslint --fix && yarn base:prettier --write -l",
    "lint-check": "yarn base:eslint && yarn base:prettier --check",
    "typecheck": "tsc --noEmit --skipLibCheck --incremental --tsBuildInfoFile node_modules/.cache/tsc/tsbuildinfo --project .",
    "markdown-lint": "remark 'content/**/docs/pages/**/*.mdx' --quiet --frail",
    "markdown-lint-external-links": "WITH_EXTERNAL_LINKS=true yarn markdown-lint",
    "markdown-fix": "FIX=true yarn markdown-lint -o",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./public/storybook/"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --check"
    ],
    "*.{css,json}": "prettier --check"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "^1.5.0",
    "@algolia/autocomplete-theme-classic": "^1.5.0",
