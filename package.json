{
  "name": "teleport-docs-next",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "git-update": "git submodule update --remote",
    "build-node": "rm -rf .build && tsc --build tsconfig.node.json",
    "add-symlinks": "node ./scripts/add-symlinks.js",
    "preflight": "yarn build-node && yarn add-symlinks",
    "dev": "yarn preflight && next dev",
    "build": "yarn preflight && next build",
    "update-and-build": "yarn git-update && yarn build",
    "start": "next start",
    "test": "jest --runInBand",
    "test:watch": "yarn test --watch",
    "base:eslint": "eslint --cache --cache-location node_modules/.cache/eslint '**/*.{js,jsx,ts,tsx}'",
    "base:prettier": "prettier '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "yarn base:eslint --fix && yarn base:prettier --write -l",
    "lint-check": "yarn base:eslint && yarn base:prettier --check",
    "typecheck": "tsc --noEmit --skipLibCheck --incremental --tsBuildInfoFile node_modules/.cache/tsc/tsbuildinfo --project .",
    "markdown-lint": "remark 'content/**/docs/pages/**/*.mdx' --frail",
    "markdown-lint-external-links": "WITH_EXTERNAL_LINKS=true yarn markdown-lint",
    "markdown-fix": "FIX=true yarn markdown-lint -o"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --check"
    ],
    "*.{css,json}": "prettier --check"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.0.0-next.9",
    "@mdx-js/react": "^2.0.0-next.9",
    "@reach/listbox": "^0.14.0",
    "@styled-system/css": "^5.1.5",
    "@types/hast": "^2.3.1",
    "docsearch.js": "^2.6.3",
    "fathom-client": "^3.0.0",
    "next": "10.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-use": "^17.2.1",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "^2.0.0-next.9",
    "@next/bundle-analyzer": "^10.0.9",
    "@svgr/webpack": "^5.5.0",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.3",
    "@types/rebass": "^4.0.8",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "ajv": "^7.2.3",
    "babel-plugin-styled-components": "^1.12.0",
    "date-fns": "^2.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-prettier-standard": "^4.0.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "estree-util-value-to-estree": "^1.2.0",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "hast-util-heading-rank": "^1.0.1",
    "hast-util-to-string": "^1.0.4",
    "jest": "^26.6.3",
    "js-yaml": "^4.0.0",
    "lint-staged": "^10.5.4",
    "mdast-util-from-markdown": "^0.8.5",
    "mdast-util-frontmatter": "^0.2.0",
    "mdast-util-gfm": "^0.1.2",
    "mdast-util-mdx": "^0.1.1",
    "micromark-extension-mdxjs": "^0.3.0",
    "prettier": "^2.2.1",
    "prettier-config-standard": "^4.0.0",
    "probe-image-size": "^7.0.1",
    "rehype-highlight": "^4.1.0",
    "rehype-slug": "^4.0.1",
    "rehype-stringify": "^8.0.0",
    "remark-cli": "9.0.0",
    "remark-copy-linked-files": "^1.5.0",
    "remark-frontmatter": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-lint-no-dead-urls": "^1.1.0",
    "remark-mdx": "^2.0.0-next.9",
    "remark-preset-lint-markdown-style-guide": "^4.0.0",
    "remark-rehype": "^8.0.0",
    "remark-validate-links": "^10.0.4",
    "simple-git-hooks": "^2.2.0",
    "stringify-object": "^3.3.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "unified": "^9.2.1",
    "unist-builder": "^2.0.3",
    "unist-util-visit": "^2.0.3",
    "url-loader": "^4.1.1",
    "vfile": "^4.2.1"
  }
}
